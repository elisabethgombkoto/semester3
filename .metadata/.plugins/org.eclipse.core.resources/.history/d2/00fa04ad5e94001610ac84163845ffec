package hue02.SortierAlgorithmen;

public class Main {

	public static void main(String[] args) {

		int[] sortieren = { 5, 8, 9, 3, 4, 2, 1, 7, 6 };
		Sort unsorted = new SelectionSort(sortieren);

		int[] sortiert = unsorted.sort(sortieren);
		Sort unsorted1 = new InsertionSort(sortieren);
		int[] sortiert1 = unsorted1.sort(sortieren);

		for (int i = 0; i < sortiert.length; i++) {
			System.out.print(sortiert[i] + ", ");

		}

	}

}
