package oo.ego8769.hue06.labirinth;

public class Field extends MapeSite {

	private MapeSite east;

	public Field() {

	}

	public void conect(Direction direction, MapeSite newNeighbor) {
		switch (direction) {
		case east:
			east = newNeighbor;
			break;
		case north:
			break;
		case south:
			break;
		case west:
			break;
		default:
			break;

		}
	}

	@Override
	public void Enter() {
		
		// TODO Auto-generated method stub
		
	}

}
