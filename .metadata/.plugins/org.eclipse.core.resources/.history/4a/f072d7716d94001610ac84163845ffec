package hue02.SortierAlgorithmen;

public class QuickSort implements Sort {

	@Override
	public Comparable[] sort(Comparable[] sortieren) {
	
		return quicksort(Comparable[] daten, 0, sortieren.length-1 );	
		
	}

	private int[] quicksort(int[] daten , int left, int right) {
		if(left<right){
			int partition= partition(int [] daten, left, right);
			quicksort(daten, left, partition-1);
			quicksort(daten, partition+1, right);
		}
		return int[] sorted;
	}
	
	

}
